{
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["api.js", "helper.js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs|html)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        }
    },
    "opts": {
        "destination": "./html",
        "recurse": false,
        "template": "node_modules/docdash"
    },
    "docdash": {
        "sort": false,
        "search": true,
        "commonNav": false,
        "wrap": true,
        "typedefs": true,
        "collapse": false,
        "removeQuotes": "none",
        "versionInOutputPath": true
    }
}

