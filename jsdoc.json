{
    "source": {
        "include": ["api.js", "helper.js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs|html)"
    },
    "opts": {
        "destination": "./html",
        "recurse": false,
        "template": "node_modules/docdash"
    },
    "templates": {
        "docdash": {
            "sort": false,
            "collapse": false,
            "wrap": false,
            "typedefs": true,
            "removeQuotes": "none"
        }
    }
}

