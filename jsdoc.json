{
    "source": {
        "include": ["api.js", "helper.js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs|html)"
    },
    "opts": {
        "destination": "./html",
        "recurse": false,
        "template": "node_modules/docdash"
    },
    "docdash": {
        "sort": false,
        "search": true,
        "commonNav": false,
        "wrap": true,
        "typedefs": false,
        "collapse": false,
        "removeQuotes": "none",
        "versionInOutputPath": true
    }
}

